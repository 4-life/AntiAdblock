{
  "manifest_version": 2,

  "name": "Anti-adblock",
  "description": "Extension for Anti-adblock",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "img/favicon-64x64.png",
    "default_popup": "popup.html",
    "default_title": "Anti-adblock"
  },
  "icons": {
      "16": "img/favicon-16x16.png",
      "48": "img/favicon-48x48.png",
      "128": "img/favicon-128x128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["style.css"],
      "js": ["userscript.js"]
    }
  ]
}
